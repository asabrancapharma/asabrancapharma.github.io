---
import SEO from '@julian_cataldo/astro-seo/SEO.astro';
import "./Layout.css";
import asaBrancaLogoSEO from './asabrancapharma-logo3.png';
export interface Props {
	title: string;
}

const { title } = Astro.props as Props;
---

<!DOCTYPE html>
<html lang="pt-br">

<head>

	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<link rel="icon" type="image/x-icon" href="/favicon.ico" />
	<title>{title}</title>

	<SEO image={asaBrancaLogoSEO} title={'Asa Branca Pharma - Conheça nossa Farmácia em Cajazeiras/PB'}
		description="Farmácia Asa Branca Pharma. Localizada próxima ao CAIC em Cajazeiras Paraíba. Faz entrega em toda Cajazeiras. Aqui você encontra os melhores preços com o melhor atendimento. Venha nos visitar!"
		siteName="Asa Branca Pharma" />


</head>

<body>
	<slot />
</body>

</html>
<style>

</style>